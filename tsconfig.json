{
  "compilerOptions": {
    "target": "es5",
    "lib": ["es5", "dom"],
    "types": ["cypress", "node"],
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "allowJs": true,
    "outDir": "./dist"
  },
  "include": [
    "cypress/**/*.ts",
    "cypress/**/*.js"
  ],
  "exclude": [
    "node_modules"
  ]
} 